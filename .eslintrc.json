{
  "extends": "@antfu",
  "rules": {
    "@typescript-eslint/indent": [
      "error",
      4
    ],
    "@typescript-eslint/brace-style": [
      0
    ],
    "@typescript-eslint/space-before-function-paren": [
      0
    ],
    "no-void": "off",
    "no-console": "warn",
    "curly": "off"
  }
}
